{
  "name": "hunydev-works",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "node scripts/generate-vscode-extensions.mjs || echo 'skip gen'",
    "dev": "vite",
    "prebuild": "node scripts/generate-vscode-extensions.mjs",
    "build": "npm run build:client && npm run build:ssr",
    "build:client": "vite build",
    "build:ssr": "vite build --ssr",
    "preview": "vite preview",
    "serve:ssr": "npm run build && wrangler dev",
    "deploy": "npm run build && wrangler deploy",
    "gen:extensions": "node scripts/generate-vscode-extensions.mjs"
  },
  "dependencies": {
    "cron-parser": "^5.4.0",
    "cronstrue": "^3.9.0",
    "dom-to-image-more": "^3.7.2",
    "highlight.js": "^11.9.0",
    "jspdf": "^2.5.2",
    "leaflet": "^1.9.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-leaflet": "^5.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250204.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wrangler": "^4.35.0"
  }
}
